<!DOCTYPE html>
<html lang="en">
#parse("content/base/back/common/head.vm")
<body style='overflow:scroll;overflow-x:hidden'>
	<div class="container bs-docs-container" style="width:100%;">
		<div class="row">
			<div class="panel panel-default">
				<div class="panel-heading">
	        	<button type="button" class="btn btn-primary" id="formSubmit"><span class="glyphicon glyphicon-ok"></span> 保存</button>
				<button type="button" class="btn btn-default" id="formReturn" data-dismiss="modal" onclick="doUrl('$!{basePath}/system/back/jwSystemAct/list.do')"><span class="glyphicon glyphicon-circle-arrow-left"></span> 返回</button>
				</div>
				<div class="panel-body">
					<form class="form-horizontal" role="form" id="dailogForm" action="$!{basePath}/system/back/jwSystemAct/doEdit.do" method="POST">
						 <input type="hidden" id="btn_sub" class="btn_sub" />
						 <input type="hidden" value="$!{jwSystemAct.id}" name="id" id="id" />
												<div class="form-group mno">
												    <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">活动项目编码</label>
												    <div class="col-sm-8">
												      <input type="text" value="$!{jwSystemAct.actId}" name="actId" id="actId" class="form-control"/>
												    </div>
												</div>
												<div class="form-group mno">
												    <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">活动名称</label>
												    <div class="col-sm-8">
												      <input type="text" value="$!{jwSystemAct.actName}" name="actName" id="actName" class="form-control"/>
												    </div>
												</div>
												<div class="form-group mno">
												    <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;"></label>
												    <div class="col-sm-8">
												      <input type="text" value="$!dateTool.format("yyyy-MM-dd",$!{jwSystemAct.beginTime})" name="beginTime" id="beginTime" class="form-control"/>
												    </div>
												</div>
												<div class="form-group mno">
												    <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;"></label>
												    <div class="col-sm-8">
												      <input type="text" value="$!dateTool.format("yyyy-MM-dd",$!{jwSystemAct.endTime})" name="endTime" id="endTime" class="form-control"/>
												    </div>
												</div>
												<div class="form-group mno">
												    <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;"></label>
												    <div class="col-sm-8">
												      <input type="text" value="$!{jwSystemAct.status}" name="status" id="status" class="form-control"/>
												    </div>
												</div>
												<div class="form-group mno">
												    <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">微信原始ID</label>
												    <div class="col-sm-8">
												      <input type="text" value="$!{jwSystemAct.jwid}" name="jwid" id="jwid" class="form-control"/>
												    </div>
												</div>
												<div class="form-group mno">
												    <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">活动链接</label>
												    <div class="col-sm-8">
												      <input type="text" value="$!{jwSystemAct.hdurl}" name="hdurl" id="hdurl" class="form-control"/>
												    </div>
												</div>
												<div class="form-group mno">
												    <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">短链接</label>
												    <div class="col-sm-8">
												      <input type="text" value="$!{jwSystemAct.shortUrl}" name="shortUrl" id="shortUrl" class="form-control"/>
												    </div>
												</div>
												<div class="form-group mno">
												    <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;"></label>
												    <div class="col-sm-8">
												      <input type="text" value="$!{jwSystemAct.organizer}" name="organizer" id="organizer" class="form-control"/>
												    </div>
												</div>
												<div class="form-group mno">
												    <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">参与数</label>
												    <div class="col-sm-8">
												      <input type="text" value="$!{jwSystemAct.joinNum}" name="joinNum" id="joinNum" class="form-control"/>
