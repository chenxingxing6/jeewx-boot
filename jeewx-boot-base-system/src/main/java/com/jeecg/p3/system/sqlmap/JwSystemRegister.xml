                     ,registertime                  /* 注册时间 */ 
                     ,lastresendtime                /* 重发时间 */ 
                     ,checksign                     /* 确认标志 */ 
					)
			values (
                      #{id,jdbcType=VARCHAR}                            /*  */ 
                     ,#{email,jdbcType=VARCHAR}                         /* 注册邮箱 */ 
                     ,#{password,jdbcType=VARCHAR}                      /* 密码 */ 
                     ,#{authstring,jdbcType=VARCHAR}                    /* 验证串 */ 
                     ,#{registertime,jdbcType=TIMESTAMP}                  /* 注册时间 */ 
                     ,#{lastresendtime,jdbcType=TIMESTAMP}                /* 重发时间 */ 
                     ,#{checksign,jdbcType=INTEGER}                     /* 确认标志 */ 
