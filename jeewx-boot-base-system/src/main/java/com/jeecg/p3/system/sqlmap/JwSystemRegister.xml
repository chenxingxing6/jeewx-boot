                   jsr.id                            /*  */ 
                  ,jsr.email                         /* 注册邮箱 */ 
                  ,jsr.password                      /* 密码 */ 
                  ,jsr.authstring                    /* 验证串 */ 
                  ,jsr.registertime                  /* 注册时间 */ 
                  ,jsr.lastresendtime                /* 重发时间 */ 
