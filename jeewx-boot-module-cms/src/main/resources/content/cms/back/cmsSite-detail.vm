<!DOCTYPE html>
<html lang="en">
#parse("content/base/back/common/head.vm")
<body style='overflow:scroll;overflow-x:hidden'>
	<div class="container bs-docs-container" style="width:100%;">
		<div class="row">
			<div class="panel panel-default">
				<div class="panel-heading">
				<button type="button" class="btn btn-default" id="formReturn" data-dismiss="modal" onclick="doUrl('$!{basePath}/cms/back/cmsSite/list.do')"><span class="glyphicon glyphicon-circle-arrow-left"></span> 返回</button>
				</div>
				  	<div class="panel-body">
				   		<form class="form-horizontal" role="form">
							  <fieldset disabled>
													 <div class="form-group mno">
													    <label for="inputEmail3" class="col-sm-1 control-label" style="text-align:left;">公司电话</label>
													    <div class="col-sm-2">
													      <input type="text" value="$!{cmsSite.companyTel}" name="companyTel" id="companyTel" class="form-control" />
													    </div>
													  </div>
													 <div class="form-group mno">
													    <label for="inputEmail3" class="col-sm-1 control-label" style="text-align:left;">站点Logo</label>
													    <div class="col-sm-2">
													      <input type="text" value="$!{cmsSite.siteLogo}" name="siteLogo" id="siteLogo" class="form-control" />
													    </div>
													  </div>
													 <div class="form-group mno">
													    <label for="inputEmail3" class="col-sm-1 control-label" style="text-align:left;">站点名称</label>
													    <div class="col-sm-2">
													      <input type="text" value="$!{cmsSite.siteName}" name="siteName" id="siteName" class="form-control" />
													    </div>
													  </div>
													 <div class="form-group mno">
													    <label for="inputEmail3" class="col-sm-1 control-label" style="text-align:left;">站点模板</label>
													    <div class="col-sm-2">
													      <input type="text" value="$!{cmsSite.siteTemplateStyle}" name="siteTemplateStyle" id="siteTemplateStyle" class="form-control" />
													    </div>
													  </div>
													 <div class="form-group mno">
													    <label for="inputEmail3" class="col-sm-1 control-label" style="text-align:left;">备案信息</label>
													    <div class="col-sm-2">
													      <input type="text" value="$!{cmsSite.recordInformation}" name="recordInformation" id="recordInformation" class="form-control" />
													    </div>
													  </div>
													 <div class="form-group mno">
													    <label for="inputEmail3" class="col-sm-1 control-label" style="text-align:left;">经度</label>
													    <div class="col-sm-2">
													      <input type="text" value="$!{cmsSite.longitude}" name="longitude" id="longitude" class="form-control" />
													    </div>
													  </div>
													 <div class="form-group mno">
													    <label for="inputEmail3" class="col-sm-1 control-label" style="text-align:left;">纬度</label>
													    <div class="col-sm-2">
													      <input type="text" value="$!{cmsSite.latitude}" name="latitude" id="latitude" class="form-control" />
													    </div>
													  </div>
							  </fieldset>
						</form>
					</div>
			</div>
		</div>
	</div>
</body>
</html>