#parse("content/base/back/common/macro.vm")
<!DOCTYPE html>
<html lang="en">
#parse("content/base/back/common/head.vm")
<link href="$!{basePath}/plug-in/plugin/hplus/css/style.css" type="text/css" rel="stylesheet"/>
<link href="$!{basePath}/plug-in/plugin/hplus/css/font-awesome.min.css" type="text/css" rel="stylesheet"/>
<body style='overflow:scroll;overflow-x:hidden'>
	<div class="container bs-docs-container" style="width:100%;">
		<div class="row">
			<form role="form" class="form-inline" action="$!{basePath}/goldeneggs/back/wxActGoldeneggsAwards/list.do?showReturnFlag=1" method="post"  id="formSubmit">
				<div  class="col-md-10" style="width:100%">
								<div class="panel panel-default">
								  <div class="panel-heading">
									#if($!{showReturnFlag})
										<button type="button" class="btn btn-default" id="formReturn" data-dismiss="modal" onclick="doUrl('$!{basePath}/goldeneggs/back/wxActGoldeneggs/list.do')"><span class="glyphicon glyphicon-circle-arrow-left"></span> 返回</button>
									#else
										砸金蛋奖项配置
									#end
								  </div>
								  <div class="panel-body">
								  		<div class="search">
															 <div class="form-group col-sm-3">
															    <label for="awardsName" class="control-label col-sm-3 line34">奖项名称</label>
															     <div class="col-sm-8">
															    <input type="text" name="awardsName" id="awardsName" value="$!{wxActGoldeneggsAwards.awardsName}" class="form-control">
															  	</div>
															 </div>
															#* <div class="form-group col-sm-3">
															    <label for="jwid" class="control-label col-sm-3 line34"></label>
															     <div class="col-sm-8">
															    <input type="text" name="jwid" id="jwid" value="$!{wxActGoldeneggsAwards.jwid}" class="form-control">
															  	</div>
															 </div>
															 <div class="form-group col-sm-3">
															    <label for="awardsValue" class="control-label col-sm-3 line34"></label>
															     <div class="col-sm-8">
															    <input type="text" name="awardsValue" id="awardsValue" value="$!{wxActGoldeneggsAwards.awardsValue}" class="form-control">
															  	</div>
															 </div>
															 <div class="form-group col-sm-3">
															    <label for="createBy" class="control-label col-sm-3 line34">创建人</label>
															     <div class="col-sm-8">
															    <input type="text" name="createBy" id="createBy" value="$!{wxActGoldeneggsAwards.createBy}" class="form-control">
															  	</div>
															 </div>*#
											  <button type="submit" class="btn btn-primary"><span class="glyphicon glyphicon-search"></span> 搜  索</button>
											  <div class="clearfix"></div>
								  		</div>
								  		<div id="legend">
								          <legend  class="le"><button type="button" class="btn btn-primary" onclick="doUrl('$!{basePath}/goldeneggs/back/wxActGoldeneggsAwards/toAdd.do?showReturnFlag=$!{showReturnFlag}')" ><span class="glyphicon glyphicon-plus"></span> 新增</button></legend> 
								        </div>
								        <table class="table table-striped">
									        <thead>
													<th>奖项名称</th>
													
													##<th>奖品序号</th>
													##<th>创建人</th>
						                			<th>操作</th>
									        </thead>
									        <tobody>
									    	  #if($!{pageInfos})
								                #foreach($!{info} in $!{pageInfos})
									            	<tr>			
																	<td>$!{info.awardsName}</td>
																	
																	##<td>$!{info.awardsValue}</td>
																	##<td>$!{info.createBy}</td>
										                <td class="last">
							                			<a class="btn btn-xs btn-primary" href="javascript:doUrl('$!{basePath}/goldeneggs/back/wxActGoldeneggsAwards/toEdit.do?id=$!{info.id}&showReturnFlag=$!{showReturnFlag}')" ><i class="fa fa-edit"></i> 编辑</a>
							                			<a class="btn btn-xs btn-danger" href="javascript:delData('$!{basePath}/goldeneggs/back/wxActGoldeneggsAwards/doDelete.do?id=$!{info.id}')"><i class="fa fa-trash-o"></i> 删除</a>

						                				</td>
									           		</tr>
									             #end
									    	 #end
									         </tobody>
									    </table>
										<div class="text-right">
											<!--公用翻页代码-->
							                #set($attr='formSubmit')
							            	#showPageList($pageInfos $attr)
							                <!--END公用翻页代码-->
										</div>
								  </div>	
								</div>
				</div>  
				</form>
		</div>
	</div>
</body>
</html>